{
  "name": "@ublacklist/builtin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "pnpm run /^check:/",
    "check:biome": "biome check",
    "check:prettier": "prettier --check **/*.{md,yaml,yml}",
    "check:tsc": "tsc",
    "fix": "pnpm run /^fix:/",
    "fix:biome": "biome check --write",
    "fix:prettier": "prettier --write **/*.{md,yaml,yml}",
    "update-searxng-instances": "node ./scripts/update-searxng-instances.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^24.10.11",
    "lefthook": "^1.13.6",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.2",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.13.0",
      "onFail": "download"
    }
  }
}
